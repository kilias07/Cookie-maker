{
  "compilerOptions": {
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ES6",
    "downlevelIteration": true,
    "lib": [
      "es6",
      "dom.iterable",
      "dom"
    ],
    "outDir": "dist",
    "types": ["whatwg-streams","whatwg-fetch"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node"
  }
}